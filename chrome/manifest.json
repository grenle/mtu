{
  "name": "mtu",
  "description": "Copy current page/title and url as markdown [title](url)",
  "version": "0.3",
  "manifest_version": 2,
  "background": {
      "page": "index.html"
  },
  "permissions": ["clipboardWrite", "activeTab", "notifications"],
  "commands": {
      "copy-pair": {
          "suggested_key": {
              "default": "Ctrl+Shift+Y"
          },
          "description": "Copy [title](url)"
      }
  }
}
